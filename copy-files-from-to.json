{
  "copyFiles": [
    {
      "from": ["src/js/*"],
      "to": {
        "development": {
          "skip": true
        },
        "default": "build/js/"
      }
    },
    {
      "from": ["src/css/*"],
      "to": {
        "development": {
          "skip": true
        },
        "default": "build/css/"
      }
    },
    {
      "from": ["src/html/*"],
      "to": {
        "development": {
          "skip": true
        },
        "default": "build/html/"
      }
    },
    {
      "from": "src/index.html",
      "to": {
        "development": {
          "skip": true
        },
        "default": "build/index.html"
      }
    },
    {
      "from": "node_modules/jquery/dist/jquery.min.js",
      "to": {
        "default":"build/js3/jquery.js",
        "development": "src/js3/jquery.js"
      }
    },
    {
      "from": "codejar/codejar.js",
      "to": {
        "default":"build/js3/codejar.js",
        "development": "src/js3/codejar.js"
      }
    },
    {
      "from": "node_modules/jquery-powertip/dist/jquery.powertip.min.js",
      "to": {
        "default":"build/js3/jquery.powertip.js",
        "development": "src/js3/jquery.powertip.js"
      }
    },
    {
      "from": "https://cdn.jsdelivr.net/gh/highlightjs/cdn-release@latest/build/highlight.min.js",
      "to": {
        "default":"build/js3/highlight.js",
        "development": "src/js3/highlight.js"
      }
    },
    {
      "from": "node_modules/advanced-mark.js/dist/mark.js",
      "to": {
        "default":"build/js3/mark.js",
        "development": "src/js3/mark.js"
      }
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite"
  }
}
